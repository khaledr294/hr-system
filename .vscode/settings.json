{
  "eslint.enable": false,
  "eslint.workingDirectories": ["."],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "cache": true,
    "cacheLocation": ".eslintcache"
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "files.exclude": {
    "**/.eslintcache": true,
    "**/node_modules": true
  },
  "editor.formatOnSave": false,
  "mssql.enabled": false,
  "files.associations": {
    "**/*.sql": "plaintext"
  }
}